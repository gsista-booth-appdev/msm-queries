<h1>List of all directors</h1>

<table border="1">
<% @list_of_directors.each do |a_director| %>
  <tr>
    <td><%= a_director.id %></td>
    <td>
      <img src="https://robohash.org/<%= a_director.name %>">
    </td>
    <td><%= a_director.name %></td>
    <td><%= a_director.bio %></td>
  </tr>
<% end %>
</table>
